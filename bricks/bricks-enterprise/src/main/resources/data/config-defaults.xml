<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE preferences SYSTEM "http://java.sun.com/dtd/preferences.dtd">
<preferences EXTERNAL_XML_VERSION="1.0">
    <root type="user">
        <map/>
        <node name="camera.defaults">
            <map>
                <entry key="camera.move.maxRadius" value="2500.0"/>
				<entry key="camera.move.speed" value="1500.0"/>
				
<!-- 				<entry key="camera.roll.speed.radians" value="3.14159"/> -->
				<entry key="camera.target.constant.rotation" value="1.571"/>
				<entry key="camera.roll.speed.radians" value="1.571"/>
				
				<entry key="camera.altitude.min" value="-200"/>
				<entry key="camera.altitude.max" value="8000"/>
				
				<entry key="camera.vertical.roll.speed" value="3.14159"/>
            </map>
        </node>
    </root>
</preferences>